Config {
  --font = "xft:anonymous pro:pixelsize=12"
  font = "xft:envypn:pixelsize=15"
  , bgColor = "#1d1f21"
  , fgColor = "#c5c8c6"
  , border = NoBorder
  , borderColor = "black"
  , hideOnStart = False
  , persistent = True
  , position = Bottom
  , pickBroadest = True
  , lowerOnStart = False
  , commands = [ Run Weather "KSFO" ["-t"," <tempC>C <skyCondition>","-L","64","-H","77","--normal","#c0c5ce","--high","#c0c5ce","--low","#c0c5ce"] 36000
               , Run DynNetwork ["-t","Net: <rxbar>|<txbar>","-S","True","-L","0","-H","1024","--normal","#c0c5ce","--high","#c0c5ce"] 10
               , Run Cpu ["-L","3","-H","50","--normal","#657b83","--high","#c0c5ce","--template","Cpu: <bar>"] 10
               , Run Memory ["-t","Mem: <usedbar>"] 10
               , Run Date "<fc=#c0c5ce>%a %b %_d %Y %H:%M</fc>" "date" 10
               , Run CommandReader "pymodoro" "pomodoro"
               , Run Com "hostname" [] "" 36000
               , Run StdinReader
--               , Run Locks
--               , Run MPD ["-t", "<state>: <artist> - <title>"] 10
               ]
  , sepChar = "%"
  , alignSep = "}{"
  , template = " %hostname% | %StdinReader% }{ %pomodoro% | %cpu% | %memory% | %dynnetwork% | SFO:%KSFO% | %date%"
  }
